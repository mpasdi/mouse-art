{
  "name": "mouse-art",
  "version": "1.0.1",
  "author": {
    "name": "mpsdi"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.mjs",
  "types": "./index.d.ts",
  "style": "dist/index.css",
  "exports": {
    "types": "./index.d.ts",
    "import": "./dist/index.mjs",
    "require": "./dist/index.umd.js"
  },
  "keywords": [
    "mouse",
    "effect"
  ],
  "description": "mouse click effects",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write src/",
    "commit": "czg",
    "lint": "eslint --max-warnings 0  \"src/**/*.{ts,tsx}\" --fix",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.2.0",
    "@types/less": "^3.0.6",
    "@types/node": "^20.12.11",
    "@types/uuid": "^9.0.8",
    "cz-git": "^1.9.1",
    "czg": "^1.9.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.2.0",
    "husky": "^8.0.0",
    "less": "^4.2.0",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.8.0",
    "vite": "^5.2.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.5",
    "animate.css": "^4.1.1",
    "redux": "^5.0.1",
    "uuid": "^9.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mpasdi/mouse-art.git"
  },
  "bugs": {
    "email": "lisongming19981@gmail.com",
    "url": "git@github.com:mpasdi/mouse-art.git/issues"
  }
